
   
  <div id="signup">
    <h1><%= title %>&#128516</h1>
    <form id="signform" action="/user/create" method="post">
        <h4>Email</h4>
        <input type="email" name="email" id="email" placeholder="xtz@gmail.com">
        <h4>Name</h4> 
        <input type="text" name="name" id="name" placeholder="Mark zukerberg">
        <h4>Password</h4>
